<%= render(MpnetworkWeb.PageView, "_breadcrumb.html", assigns: Enum.into(@conn.assigns, %{breadcrumb_path: ~w[ Listings New ]})) %>


<%= render "form.html", changeset: @changeset,
                        action: Routes.listing_path(@conn, :create),
                        conn: @conn,
                        attachments: [],
                        offices: @offices,
                        users: @users,
                        existing: false %>
<p><i>Attachments can be added after saving the listing</i></p>
<span><%= link "Back", to: Routes.listing_path(@conn, :index) %></span>
